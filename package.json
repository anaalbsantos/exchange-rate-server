{
  "name": "boilerplate-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "engines": {
    "node": ">=18.16"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@prisma/client": "5.20.0",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.36",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-winston": "^4.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "prisma": "^5.21.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "scripts": {
    "build": "swc src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "npm run build && node dist/server.js",
    "dev": "concurrently \"ts-node-dev --poll --exit-child -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts\" \"prisma generate --watch\"",
    "prod": "npm run migration:deploy && npm run start",
    "migration": "cross-env DATABASE_HOST='localhost' prisma migrate dev",
    "migration:deploy": "prisma migrate deploy",
    "migration:reset": "cross-env DATABASE_HOST='localhost' prisma migrate reset --force",
    "studio": "cross-env DATABASE_HOST='localhost' prisma studio",
    "generate": "prisma generate",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "push": "cross-env DATABASE_HOST='localhost' prisma db push",
    "test:ci": "cross-env DATABASE_URL='postgres://postgres:postgres@localhost:5432/postgres' prisma db push && cross-env JWT_REFRESH_SECRET=CITi JWT_ACCESS_SECRET=CITi DATABASE_URL='postgres://postgres:postgres@localhost:5432/postgres' jest --runInBand --ci",
    "test": "cross-env DATABASE_URL='postgres://postgres:docker@localhost:5433/boilerplate-test' prisma db push && cross-env DATABASE_URL='postgres://postgres:docker@localhost:5433/boilerplate-test' jest --runInBand",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "22.7.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prisma-dbml-generator": "^0.12.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
